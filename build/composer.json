{
    "authors": [
        {
            "name": "UNB Libraries libsupport@unb.ca",
            "role": "Creator"
        }
    ],
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        }
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "description": "Drupal 8 component of lib.unb.ca",
    "extra": {
        "enable-patching": true,
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        },
        "patches": {}
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "name": "unb-libraries/lib.unb.ca",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "farbelous/fontawesome-iconpicker",
                "version": "1.3.1",
                "type": "drupal-library",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/farbelous/fontawesome-iconpicker/archive/1.3.1.zip"
                },
                "require": {
                    "composer/installers": "^1.2.0"
                }
            }
        }
    ],
    "require": {
        "drupal/bootstrap_barrio": "4.21",
        "drupal/config_update": "1.5",
        "drupal/core": "8.6.9",
        "drupal/externalauth": "1.0",
        "drupal/features": "3.7",
        "farbelous/fontawesome-iconpicker": "~1.3",
        "drupal/fontawesome": "2.9",
        "drupal/fontawesome_iconpicker": "1.0-beta1",
        "drupal/fontawesome_menu_icons": "1.5",
        "drupal/google_analytics": "2.2",
        "drupal/ldap": "3.0-beta3",
        "drupal/pathauto": "1.1",
        "drupal/paragraphs": "1.5",
        "drupal/redis": "1.0-rc2",
        "drupal/search_api": "1.11",
        "drupal/search_api_autocomplete": "1.1",
        "drupal/search_api_solr": "2.3",
        "drupal/token": "1.0",
        "unb-libraries/drupal-builder": "^8.0.0"
    },
    "require-dev": {
        "unb-libraries/drupal-development-libraries": "^8.0.0"
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ]
    },
    "type": "project"
}
