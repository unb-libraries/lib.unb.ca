                "dist": {
                    "type": "zip",
                    "url": "https://github.com/FortAwesome/Font-Awesome/archive/5.14.0.zip"
                },
                "extra": {
                    "installer-name": "fontawesome"
                }
            }
        },
        {
            "type": "package",
