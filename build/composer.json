{
    "authors": [
        {
            "name": "UNB Libraries libsupport@unb.ca",
            "role": "Creator"
        }
    ],
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        }
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "description": "Drupal 8 component of lib.unb.ca",
    "extra": {
        "enable-patching": true,
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        },
        "patches": {
            "drupal/bootstrap_barrio": {
                "Fix CSS library 404": "https://www.drupal.org/files/issues/2019-02-19/3033946-6.patch",
                "Fix Layout Builder": "https://www.drupal.org/files/issues/2018-06-01/2976934-3.patch"
            },
            "drupal/embed": {
                "Allow non-managed button icon file": "https://www.drupal.org/files/issues/2018-09-25/embed-allow-non-managed-button-icon-files-2824110-19.patch"
            },
            "drupal/entity_browser": {
                "Provide upload widget for media of media type file": "https://www.drupal.org/files/issues/2019-02-28/entity_browser-media-file-upload-2922885-15.patch"
            }
        }
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "name": "unb-libraries/lib.unb.ca",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor/colorbutton",
                "version": "4.11.4",
                "type": "drupal-library",
                "dist": {
                    "type": "zip",
                    "url": "https://download.ckeditor.com/colorbutton/releases/colorbutton_4.11.4.zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor/div",
                "version": "4.11.4",
                "type": "drupal-library",
                "dist": {
                    "type": "zip",
                    "url": "https://download.ckeditor.com/div/releases/div_4.11.4.zip"
                },
                "extra": {
                    "installer-name": "ckeditor/plugins/div"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor/panelbutton",
                "version": "4.11.4",
                "type": "drupal-library",
                "dist": {
                    "type": "zip",
                    "url": "https://download.ckeditor.com/panelbutton/releases/panelbutton_4.11.4.zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "enyo/dropzone",
                "version": "5.5.0",
                "type": "drupal-library",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/enyo/dropzone/archive/v5.5.0.zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "farbelous/fontawesome-iconpicker",
                "version": "1.3.1",
                "type": "drupal-library",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/farbelous/fontawesome-iconpicker/archive/1.3.1.zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/harvesthq/chosen/releases/download/v1.8.7/chosen_v1.8.7.zip"
                },
                "name": "harvesthq/chosen",
                "require": {
                    "composer/installers": "^1.2.0"
                },
                "type": "drupal-library",
                "version": "1.8.7"
            }
        },
        {
            "type": "package",
            "package": {
                "name": "unb-libraries/calendar_hours",
                "version": "dev-8.x-1.x",
                "type": "drupal-module",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/unb-libraries/calendar_hours/archive/8.x-1.x.zip"
                }
            }
        }
    ],
    "require": {
        "ckeditor/colorbutton": "~4.11",
        "ckeditor/div": "~4.11",
        "ckeditor/panelbutton": "~4.11",
        "drupal/allowed_formats": "1.1",
        "drupal/block_class": "1.0",
        "drupal/block_field": "1.0-alpha8",
        "drupal/bootstrap_barrio": "4.22",
        "drupal/chosen": "2.7",
        "drupal/ckeditor_div_manager": "1.0",
        "drupal/colorbutton": "1.1",
        "drupal/config_update": "1.5",
        "drupal/core": "8.7.6",
        "drupal/easy_breadcrumb": "1.12",
        "drupal/entity_browser": "2.2",
        "drupal/entity_embed": "1.0",
        "drupal/externalauth": "1.1",
        "drupal/features": "3.7",
        "farbelous/fontawesome-iconpicker": "~1.3",
        "drupal/fontawesome": "2.13",
        "drupal/fontawesome_iconpicker": "1.0-beta1",
        "drupal/google_analytics": "2.4",
        "drupal/honeypot": "1.29",
        "drupal/inline_entity_form": "1.0-rc1",
        "drupal/ldap": "3.0-beta7",
        "drupal/linkit": "5.0-beta9",
        "drupal/media_entity_browser": "2.0-alpha2",
        "drupal/noreqnewpass": "1.1",
        "drupal/override_node_options": "2.4",
        "drupal/panelbutton": "1.2",
        "drupal/pathauto": "1.4",
        "drupal/paragraphs": "1.8",
        "drupal/redis": "1.1",
        "drupal/scroll_back_to_top": "1.0-rc1",
        "drupal/search_api": "1.12",
        "drupal/search_api_autocomplete": "1.2",
        "drupal/search_api_solr": "2.6",
        "drupal/token": "1.5",
        "drupal/webform": "5.3",
        "harvesthq/chosen": "~1.8",
        "unb-libraries/calendar_hours": "dev-8.x-1.x",
        "unb-libraries/drupal-builder": "^8.0.0",
        "unb-libraries/unb_lib_theme": "dev-8.x-bootstrap-4.x"
    },
    "require-dev": {
        "unb-libraries/drupal-development-libraries": "^8.0.0"
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ]
    },
    "type": "project"
}
